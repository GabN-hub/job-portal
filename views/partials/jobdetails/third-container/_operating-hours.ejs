<div class="third-container mb-8 h-fit w-full featured-jobs-card px-4">
  <div class="header flex items-center gap-3 border-b border-grayBorder py-3">
    <div class="bg-btngreen w-9 h-9 rounded-full flex items-center justify-center"><%- mainIcon %></div>
    <h3 class="font-poppins font-semibold text-lg text-title"><%= title %></h3>
  </div>
  <div class="px-2">
    <ul>
      <% items.forEach(item => { %>
      <li class="flex justify-between py-2  text-description font-medium">
        <p><%= item.day %></p>
        <p><%= item.time %></p>
      </li>
      <% }); %>
    </ul>
  </div>
</div>
